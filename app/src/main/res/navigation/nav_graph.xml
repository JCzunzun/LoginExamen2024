<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentLogin">

    <fragment
        android:id="@+id/fragmentRegister2"
        android:name="edu.iesam.loginexam1eval.presentation.FragmentRegister"
        android:label="FragmentRegister" >
        <action
            android:id="@+id/action_fragmentRegister2_to_fragmentWelcome"
            app:destination="@id/fragmentWelcome" />
    </fragment>
    <fragment
        android:id="@+id/fragmentLogin"
        android:name="edu.iesam.loginexam1eval.presentation.FragmentLogin"
        android:label="FragmentLogin" >
        <action
            android:id="@+id/action_fragmentLogin_to_fragmentRegister2"
            app:destination="@id/fragmentRegister2" />
        <action
            android:id="@+id/action_fragmentLogin_to_fragmentWelcome"
            app:destination="@id/fragmentWelcome" />
        <action
            android:id="@+id/action_fragmentLogin_to_fragmentUnsuscribe"
            app:destination="@id/fragmentUnsuscribe" />
    </fragment>
    <fragment
        android:id="@+id/fragmentWelcome"
        android:name="edu.iesam.loginexam1eval.presentation.FragmentWelcome"
        android:label="FragmentWelcome" />
    <fragment
        android:id="@+id/fragmentUnsuscribe"
        android:name="edu.iesam.loginexam1eval.presentation.FragmentUnsuscribe"
        android:label="FragmentUnsuscribe" >
        <action
            android:id="@+id/action_fragmentUnsuscribe_to_fragmentLogin"
            app:destination="@id/fragmentLogin" />
    </fragment>
</navigation>